{
    // 指定第三方库的代码提示和自动补全
    // 【改】第三方库的目录
    "python.autoComplete.extraPaths": [
        "./AppData/.site-packages"
    ],
    "python.analysis.extraPaths": [
        "./AppData/.site-packages"
    ],
    "editor.suggest.showWords": true,
    "cmake.configureOnOpen": false,
    // 隐藏文件和目录 
    "files.exclude": {
        "**/__pycache__": true,
        "**/.runtime": true,
        "**/**.exe": true
    },
}